<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liu.mapper.regMapper">
    <!--    名称空间-->
    <insert id="insert" >
       insert into stu(username,number,name,xueyan,zhany,nianji,banji,pwd)
values (#{username},#{number},#{name},#{xueyan},#{zhany},#{nianji},#{banji},#{pwd})

    </insert>

</mapper>
